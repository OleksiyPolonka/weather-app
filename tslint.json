{
  "extends": [
    "tslint-react",
    "tslint:recommended",
    "tslint-config-airbnb",
    "tslint-config-prettier"
  ],
  "rules": {
    "import-name": false,
    "function-name": false,
    "quotemark": [
      true,
      "single",
      "jsx-double",
      "avoid-template"
    ],
    "jsx-boolean-value": [false],
    "variable-name": [false]
  }
}